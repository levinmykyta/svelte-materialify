{
  "name": "svelte-materialify",
  "version": "0.1.9",
  "description": "A material design framework/component library for Svelte, with full support for light and dark themes and customisability.",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "watch": "concurrently \"yarn:watch:*\" -k",
    "start": "concurrently \"yarn:build\" \"yarn:css\"",
    "build": "rollup -c",
    "watch:build": "rollup -c -w",
    "css": "sass src/styles/_global.scss:dist/global.css -I theme -s compressed --no-source-map",
    "watch:css": "yarn run css --watch",
    "test": "jest",
    "check": "svelte-check --workspace src/components",
    "version": "standard-changelog && git add CHANGELOG.md",
    "prepublishOnly": "concurrently \"yarn:build\" \"yarn:css\""
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.2.2",
    "concurrently": "^5.3.0",
    "jest": "^26.4.2",
    "postcss": "^7.0.34",
    "rollup": "^2.28.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.26.10",
    "standard-changelog": "^2.0.24",
    "svelte": "^3.28.0",
    "svelte-check": "^1.0.52",
    "svelte-htm": "^1.1.1",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.3.2"
  },
  "files": [
    "src",
    "dist",
    "@types"
  ],
  "peerDependencies": {
    "sass": "^1.26.10",
    "svelte-preprocess": "^4.0.10"
  },
  "peerDependenciesMeta": {
    "sass": {
      "optional": true
    },
    "svelte-preprocess": {
      "optional": true
    }
  },
  "dependencies": {
    "uid": "^1.0.0"
  },
  "author": "TheComputerM",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/TheComputerM/svelte-materialify",
    "type": "git",
    "directory": "packages/svelte-materialify"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "svelte-components",
    "material-design",
    "material",
    "material-theme",
    "material-components",
    "component-library",
    "svelte-material",
    "ui",
    "ui-components",
    "ui-kit"
  ]
}
