{
  "name": "svelte-materialify",
  "version": "0.0.1",
  "svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm:watch:*\" -k",
    "start": "concurrently \"npm:build\" \"npm:css\" \"npm:copy-css\" \"npm:site:export\" -k",
    "build": "rollup -c",
    "watch:build": "rollup -c -w",
    "css": "sass src/styles/_global.scss:dist/global.css -I theme -s compressed --no-source-map",
    "watch:css": "npm run css -- --watch",
    "copy-css": "copy-and-watch dist/global.css site/static",
    "watch:copy-css": "npm run copy-css -- --watch",
    "test": "jest",
    "site:dev": "cd site && npm run dev",
    "site:build": "cd site && npm run build",
    "site:export": "npm run copy-css && cd site && npm run export",
    "deploy": "npm run site:export && node deploy.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/svelte": "^3.0.0",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.2.2",
    "concurrently": "^5.2.0",
    "copy-and-watch": "^0.1.5",
    "del": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "ncp": "^2.0.0",
    "postcss": "^7.0.32",
    "rollup": "^2.23.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "simple-git": "^2.15.0",
    "svelte-jester": "^1.0.6"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "material-ripple-effect": "^1.0.3",
    "sass": "^1.26.10",
    "svelte": "^3.24.0",
    "svelte-preprocess": "4.0.8"
  },
  "author": "TheComputerM",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/TheComputerM/svelte-materialify",
    "type": "git"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "svelte-components",
    "material-design",
    "material",
    "material-theme",
    "material-components",
    "component-library",
    "svelte-material",
    "ui",
    "ui-components",
    "ui-kit"
  ]
}
